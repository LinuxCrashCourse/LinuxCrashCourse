#include <iostream>
using namespace std;
uint64_t f(int n) {
	uint64_t sum = 0;
	for (uint32_t i = 1; i <= n; i++)
		sum += i*sum;
  return sum;	
}

uint64_t g(int n) {
	uint64_t sum = 0;
	for (uint32_t i = 1; i <= n; i++)
		for (uint32_t j = 1; j <= i; j++)
			sum += j*sum;
  return sum;	
}

int main(int argc, char* argv[]) {
	int n = argc >= 1 ? atoi(argv[1]) : 1000000;

	uint64_t sum = 0;
	sum += f(n);
	sum += g(n);
	cout << sum << '\n';
}
